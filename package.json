{
  "name": "mycommunity",
  "version": "1.0.9",
  "description": "My Community Project.",
  "main": "index.js",
  "scripts": {
    "start": "node-dev --no-deps --no-warnings index.js",
    "check": "tsc --noEmit",
    "logs": "docker exec -it backend pm2 logs",
    "logs:beta": "docker exec -it backend-beta pm2 logs",
    "start:prod": "node build/compiled.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "deploy": "npm run build && docker-compose up --build -d my-community",
    "deploy:force": "git pull origin master --force && npm run build && docker-compose up --build -d my-community",
    "deploy:beta:force": "git pull origin beta --force && npm run build && docker-compose up --build -d my-community-beta",
    "build": "webpack --config webpack.config.prod.js",
    "start:hmr": "node dist/server",
    "webpack": "webpack --config webpack.config.dev.js"
  },
  "engines": {
    "node": ">= 9.2",
    "npm": ">= 6.1.0"
  },
  "author": "Shady Khalifa <shekohex@gmail.com>",
  "license": "MIT",
  "private": true,
